{
  "name": "gowin",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint:check": "eslint --ignore-path .eslintignore --ext .js,.jsx,.ts .",
    "lint:fix": "eslint . --fix",
    "prettier:check": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\"",
    "lint-prettier": "yarn lint:check && yarn prettier:check",
    "prepare": "husky install"
  },
  "lint-staged": {
    "**/*.{js,jsx}": [
      "yarn lint-prettier", "yarn lint:fix"
    ]
  },
  "dependencies": {
    "@ant-design/cssinjs": "^1.17.2",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "antd": "^5.10.1",
    "eslint-config-prettier": "^9.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.0.1",
    "next": "13.5.6",
    "prettier": "^3.0.3",
    "react": "^18",
    "react-dom": "^18",
    "typescript": "^5.2.2"
  },
  "devDependencies": {
    "autoprefixer": "^10",
    "eslint": "^8",
    "eslint-config-next": "13.5.6",
    "postcss": "^8",
    "tailwindcss": "^3"
  }
}
